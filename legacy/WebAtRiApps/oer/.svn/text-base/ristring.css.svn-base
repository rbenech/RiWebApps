/* CSS style sheet to support use of ristrings (RiListString) */

/*Colors used by RiStrings (i.e. RiListStrings):*/
.rsColor1  { color:#000000; } /*Black*/ 
.rsColor2  { color:#404040; } /*DarkGray*/
.rsColor3  { color:#808080; } /*LightGray*/
.rsColor4  { color:#ffffff; } /*White*/
.rsColor5  { color:#dc0000; } /*LightRed*/
.rsColor6  { color:#00dc00; } /*LightGreen*/
.rsColor7  { color:#0000dc; } /*LightBlue*/
.rsColor8  { color:#dcdc00; } /*LightYellow*/
.rsColor9  { color:#dc00dc; } /*LightMagenta*/
.rsColor10 { color:#00dcdc; } /*LightCyan*/
.rsColor11 { color:#800000; } /*DarkRed*/
.rsColor12 { color:#008000; } /*DarkGreen*/
.rsColor13 { color:#000080; } /*DarkBlue*/
.rsColor14 { color:#808000; } /*DarkYellow*/
.rsColor15 { color:#800080; } /*DarkMagenta*/

/*Font styles used by RiStrings (i.e. RiListStrings):*/
.rsStyle1  { font-weight:bold   } /*bold*/
.rsStyle2  { font-style:italic  } /*italic*/
.rsStyle3  { font-weight:bold font-style:italic } /*bold italic*/
